#!/usr/bin/env node
var path = require('path')
var fs = require('fs')
var lib = path.join(
  path.dirname(fs.realpathSync(__filename)),
  '..', 'source', 'index'
)
require(lib)(
  process.stdin,
  process.stdout,
  process.stderr,
  process.env,
  process.argv.slice(2),
  function (status) {
    process.exit(status)
  }
)
